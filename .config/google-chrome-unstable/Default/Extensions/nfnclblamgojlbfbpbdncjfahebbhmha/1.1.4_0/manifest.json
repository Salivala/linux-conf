{
   "background": {
      "scripts": [ "js/tabitha-back-compiled.js" ]
   },
   "browser_action": {
      "default_icon": "tab19.png",
      "default_name": "Tabitha",
      "default_popup": "tabitha-pop.html",
      "default_title": "Tabitha"
   },
   "commands": {
      "_execute_browser_action": {
         "suggested_key": {
            "chromeos": "Alt+T",
            "linux": "Alt+T",
            "mac": "Command+Shift+T",
            "windows": "Alt+T"
         }
      },
      "close": {
         "description": "Quick close",
         "suggested_key": {
            "chromeos": "Alt+X",
            "linux": "Alt+X",
            "mac": "MacCtrl+X",
            "windows": "Alt+X"
         }
      },
      "combine": {
         "description": "Quick combine",
         "suggested_key": {
            "chromeos": "Alt+C",
            "linux": "Alt+C",
            "mac": "MacCtrl+C",
            "windows": "Alt+C"
         }
      },
      "pin": {
         "description": "Quick pin",
         "suggested_key": {
            "chromeos": "Alt+P",
            "linux": "Alt+P",
            "mac": "MacCtrl+P",
            "windows": "Alt+P"
         }
      }
   },
   "description": "Own your tabs.",
   "icons": {
      "128": "tab128.png",
      "16": "tab16.png",
      "48": "tab48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Tabitha Tab Manager",
   "options_page": "tabitha-opt.html",
   "permissions": [ "tabs", "storage", "bookmarks" ],
   "short_name": "Tabitha",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.4"
}
